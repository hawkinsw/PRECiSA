cpr_lat_enc_single: THEORY
BEGIN
IMPORTING float@aerr754sp

modu(A, B: unb_single): unb_single = Ssub(A, Smul(B, Sfloor(Sdiv(A,B))))

yz0 (Lat: unb_single): unb_single =
    Sfloor(Sadd(Smul(Sdiv(modu(Lat, Sdiv(RtoS(360), RtoS(60))), Sdiv(RtoS(360), RtoS(60))), RtoS(131072)), RtoS(0.5)))

yz1 (Lat: unb_single): unb_single =
    Sfloor(Sadd(Smul(Sdiv(modu(Lat, Sdiv(RtoS(360), RtoS(59))), Sdiv(RtoS(360), RtoS(59))), RtoS(131072)), RtoS(0.5)))    

END cpr_lat_enc_single