cd2d_tauB_double: THEORY
BEGIN
IMPORTING float@aerr754dp

max (A,B: unb_double):unb_double = IF (A>B) THEN A ELSE B ENDIF

min (A,B: unb_double):unb_double = IF (A<B) THEN A ELSE B ENDIF

tau (Sx, Sy, Vx, Vy, T, B: unb_double): unb_double =
   min (max (Dmul(B,Dadd (Dmul(Vx, Vx),Dmul(Vy, Vy))), Dneg(Dadd (Dmul (Sx, Vx), Dmul (Sx, Vx)))), Dmul(T, (Dadd (Dmul(Vx, Vx),Dmul(Vy, Vy)))))

END cd2d_tauB_double