if_single: THEORY
BEGIN
IMPORTING float@aerr754sp

f (X, Y: unb_single): unb_single = IF (NOT (X = Sadd(X, Y)))
                                       THEN IF (Y = X)
				            THEN (Sadd(X, Y))
					    ELSE (Sadd(Y, Y))
					    ENDIF
                                       ELSE (Smul(X, Y))
                                   ENDIF
END if_single