entrypoints Spec;{- Specification -}token VarId       ( (upper) (letter | digit | '_' | '?')* ) ;token NonVarId    ( (lower) (letter | digit | '_' | '?')* ) ;LBInt.       LBound ::= Integer;LBDouble.    LBound ::= Double;LBNegInt.    LBound ::= "-" Integer;LBNegDouble. LBound ::= "-" Double;LInf.        LBound ::= "-inf";UBInt.       UBound ::= Integer;UBDouble.    UBound ::= Double;UBNegInt.    UBound ::= "-" Integer;UBNegDouble. UBound ::= "-" Double;UInf.        UBound ::= "+inf";separator nonempty VarBind "," ;VarSpec. VarBind ::= VarId "in" "[" LBound "," UBound "]";separator nonempty VarId "," ;SpecBindN. SpecBind ::= NonVarId "(" [VarId] ")" ":" [VarBind] ;SpecBind0. SpecBind ::= NonVarId ":" [VarBind] ;separator nonempty SpecBind "" ;Specification. Spec ::= [SpecBind] ;