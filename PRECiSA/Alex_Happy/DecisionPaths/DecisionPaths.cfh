entrypoints TargetDPs;{- TargetDPs -}token VarId       ( (upper) (letter | digit | '_' | '?')* ) ;token NonVarId    ( (lower) (letter | digit | '_' | '?')* ) ;Decision.   Decision ::= Integer;separator Decision "," ;DecisionPath. DecisionPath ::= [Decision];separator DecisionPath "," ;DecisionPathsList. DecisionPaths ::= "[" [DecisionPath] "]";DecisionPathsAll.  DecisionPaths ::= "All" ;DecisionPathsNone. DecisionPaths ::= "None" ;separator nonempty VarId "," ;DecPathBindN. DecPathBind ::= NonVarId "(" [VarId] ")" ":" DecisionPaths ;DecPathBind0. DecPathBind ::= NonVarId ":" DecisionPaths ;separator nonempty DecPathBind "" ;TargetDPs. TargetDPs ::= [DecPathBind] ;